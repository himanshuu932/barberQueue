{
  "expo": {
    "name": "Numbr",
    "slug": "barber",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "projectId": "f88ea018-d6e1-476b-9d0d-ab96e34e3376",
    "icon": "./assets/images/icon.jpg",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.x2023021235.barber",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyB_qepC3UG7dcdWG645NtYZHa3aserIJoY"
        }
      },
      "googleServicesFile": "./google-services.json"
    },
   "notification": {
      "icon": "./assets/images/ic_stat_ic_stat_noti.png"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-notifications",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "cover",
          "backgroundColor": "#f9fafb"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "googleMapsApiKey": "AIzaSyB_qepC3UG7dcdWG645NtYZHa3aserIJoY",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f88ea018-d6e1-476b-9d0d-ab96e34e3376"
      }
    }
  }
}
